<configs>
    <append xpath="/windows">
        <window name="caveConfigWindow" pos="0,0" controller="XUiC_CavesConfig, ProceduralCaves">
            <rect depth="0" anchor="RightTop" pos="400, 300">
                <sprite depth="1" name="background" color="[darkGrey]" type="sliced" width="390" height="700" pos="0,-3" />
                <grid cols="1"  cell_width="390" cell_height="46" arrangement="vertical" repeat_content="false">
                    <rect name="seed">
                        <label name="label" depth="4" width="100" pos="10, -10" height="30" style="hover" text="seed" font_size="28" color="[white]" justify="left" />
                        <textfield name="input" depth="5" pos="150, -10" height="30"  width="150"  label="coucou"/>
                    </rect>
                    <rect name="Threshold">
                        <label name="label" depth="4" width="100" pos="10, -10" height="30" style="hover" text="Threshold" font_size="28" color="[white]" justify="left" />
                        <textfield name="input" depth="5" pos="150, -10" height="30"  width="150"  label="coucou"/>
                    </rect>
                    <rect name="invert">
                        <label name="label" depth="4" width="100" pos="10, -10" height="30" style="hover" text="invert" font_size="28" color="[white]" justify="left" />
                        <combobox name="input" type="ComboBoxBool" value_wrap="true" pos="150, -10" width="150" height="30" />
                    </rect>
                    <rect name="LacunarityY">
                        <label name="label" depth="4" width="100" pos="10, -10" height="30" style="hover" text="LacunarityY" font_size="28" color="[white]" justify="left" />
                        <textfield name="input" depth="5" pos="150, -10" height="30"  width="150"  label="coucou"/>
                    </rect>
                    <rect name="GainY">
                        <label name="label" depth="4" width="100" pos="10, -10" height="30" style="hover" text="GainY" font_size="28" color="[white]" justify="left" />
                        <textfield name="input" depth="5" pos="150, -10" height="30"  width="150"  label="coucou"/>
                    </rect>
                    <rect name="FrequencyY">
                        <label name="label" depth="4" width="100" pos="10, -10" height="30" style="hover" text="FrequencyY" font_size="28" color="[white]" justify="left" />
                        <textfield name="input" depth="5" pos="150, -10" height="30"  width="150"  label="coucou"/>
                    </rect>
                </grid>
            </rect>
        </window>
    </append>
</configs>