<configs>

    <!-- meleeToolFlashlight02 -->
    <append xpath="/items/item[@name='meleeToolFlashlight02']">
        <property name="ShowQuality" value="true" />
        <property name="RepairTools" value="battery_T1" />
        <property name="DegradationBreaksAfter" value="false" />

        <effect_group name="flashLightBatteryConsuming">
            <passive_effect name="DegradationMax" operation="base_set" value="300,600,900,1200,1500,1800" tier="1,2,3,4,5,6" />

            <triggered_effect trigger="onSelfItemActivate" action="AddBuff" buff="buffDecayItemOverTime" />
            <triggered_effect trigger="onSelfItemDeactivate" action="RemoveBuff" buff="buffDecayItemOverTime" />
            <triggered_effect trigger="onSelfEquipStop" action="RemoveBuff" buff="buffDecayItemOverTime" />
        </effect_group>
    </append>

